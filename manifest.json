{
  "name": "Socialblade Scraper Plugin",
  "description": "This extension will scrape a Socialblade page.",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
